param N := 3;

param A {1..4};
param D {1..4};
param G{1..4,1..3};
param B{1..4,1..3};

var x{1..N} := 0.1;
var F{i in 1..4} = A[i] + sum {j in 1..3} G[i,j]*x[j] + 0.5*(sum {j in 1..3} (x[j]*sum {k in 1..3} B[i,k]*x[k]))*D[i];

minimize f:
sum {i in 1..4} F[i]^2;

data;
